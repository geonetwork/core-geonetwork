	<dc:title>Discipline</dc:title>
	<dc:description>Origin of data among in-situ or satellite observation or numerical
            models.</dc:description>
	<dcterms:issued>2008-06-01</dcterms:issued>
	<dcterms:modified>2008-06-01</dcterms:modified>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/myocean/ontology/vocabulary/discipline#in-situ-observation">
