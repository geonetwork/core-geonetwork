<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:dcterms="http://purl.org/dc/terms/" xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:exslt="http://exslt.org/common"
    xmlns:gemet="http://www.eionet.europa.eu/gemet/2004/06/gemet-schema.rdf#">
    <skos:ConceptScheme rdf:about="http://purl.org/myocean/ontology/vocabulary/temporal-scale#">
        <dc:title>Temporal scale</dc:title>
        <dc:description>Temporal scale or domain.</dc:description>
        <dcterms:issued>2009-06-12</dcterms:issued>
        <dcterms:modified>2009-06-12</dcterms:modified>
    </skos:ConceptScheme>
    <skos:Concept rdf:about="http://purl.org/myocean/ontology/vocabulary/temporal-scale#invariant*">
        <skos:prefLabel xml:lang="en">invariant*</skos:prefLabel>
        <skos:definition xml:lang="en">Reference datasets (e.g. bathymetry) (obselete)</skos:definition>
        <skos:prefLabel xml:lang="fr">invariant*</skos:prefLabel>
        <skos:definition xml:lang="fr">Reference datasets (e.g. bathymetry) (obselete)</skos:definition>
    </skos:Concept>
    <skos:Concept rdf:about="http://purl.org/myocean/ontology/vocabulary/temporal-scale#invariant">
        <skos:prefLabel xml:lang="en">invariant</skos:prefLabel>
        <skos:definition xml:lang="en">Reference datasets (e.g. bathymetry)</skos:definition>
        <skos:prefLabel xml:lang="fr">invariant</skos:prefLabel>
        <skos:definition xml:lang="fr">Reference datasets (e.g. bathymetry)</skos:definition>
    </skos:Concept>
    <skos:Concept rdf:about="http://purl.org/myocean/ontology/vocabulary/temporal-scale#multi-year">
        <skos:prefLabel xml:lang="en">multi-year</skos:prefLabel>
        <skos:definition xml:lang="en">Multi-year analysis updated on a yearly basis.</skos:definition>
        <skos:prefLabel xml:lang="fr">multi-year</skos:prefLabel>
        <skos:definition xml:lang="fr">Multi-year analysis updated on a yearly basis.</skos:definition>
    </skos:Concept>
    <skos:Concept rdf:about="http://purl.org/myocean/ontology/vocabulary/temporal-scale#near-real-time">
        <skos:prefLabel xml:lang="en">near-real-time</skos:prefLabel>
        <skos:definition xml:lang="en">Updated daily to n-daily. Latest information may be more than
            n-day old.</skos:definition>
        <skos:prefLabel xml:lang="fr">near-real-time</skos:prefLabel>
        <skos:definition xml:lang="fr">Updated daily to n-daily. Latest information may be more than
            n-day old.</skos:definition>
    </skos:Concept>
    <skos:Concept rdf:about="http://purl.org/myocean/ontology/vocabulary/temporal-scale#forecast">
        <skos:prefLabel xml:lang="en">forecast</skos:prefLabel>
        <skos:definition xml:lang="en">Provides information for the future.</skos:definition>
        <skos:prefLabel xml:lang="fr">forecast</skos:prefLabel>
        <skos:definition xml:lang="fr">Provides information for the future.</skos:definition>
    </skos:Concept>
	<skos:Concept rdf:about="http://purl.org/myocean/ontology/vocabulary/temporal-scale#nrt-forecast">
        <skos:prefLabel xml:lang="en">nrt-forecast</skos:prefLabel>
        <skos:definition xml:lang="en">Provides information for the future or near-real time (interim keyword, used until a product can have multiple temporal scales, obsolete after)</skos:definition>
        <skos:prefLabel xml:lang="fr">nrt-forecast</skos:prefLabel>
        <skos:definition xml:lang="fr">Provides information for the future or near-real time (interim keyword, used until a product can have multiple temporal scales, obsolete after)</skos:definition>
    </skos:Concept>
</rdf:RDF>
