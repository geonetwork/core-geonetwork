	<setting name="autodetect" label="Language detection"/>
	<setting name="system/autodetect/enable" label="Enable auto-detecting search request language">
		If set, the search language is automatically detected. Note that this doesn't work very well for short search terms.
	</setting>
	
	<setting name="autofixing" label="Metadata update"/>
	<setting name="system/autofixing/enable" label="Automatic Fixes">
		When enabled, GeoNetwork automatically applies certain fixes when you save metadata.
	</setting>
	
	<setting name="clickablehyperlinks" label="Link in metadata records"/>
	<setting name="system/clickablehyperlinks/enable" label="Clickable hyperlinks">If set, GeoNetwork will display clickable hyperlinks in the metadata.</setting>
	
	
	<setting name="csw" label="Catalog Service for the Web (CSW)"/>
