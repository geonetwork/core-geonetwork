<setting label="Włącz wykrywanie języka w wyszukiwaniu" name="system/autodetect/enable">
			If set, the search language is automatically detected. Note that this doesn't work very well for short search terms.
		</setting>
<setting label="Automatyczne poprawki" name="autofixing"/>
<setting label="Włącz" name="system/autofixing/enable">
			Jeśli włączone GeoNetwork automatycznie dokonuje korekt przy zapisie metadanych.
		</setting>
<setting label="Aktywne hyperlinki" name="clickablehyperlinks"/>
<setting label="Włącz" name="system/clickablehyperlinks/enable">
