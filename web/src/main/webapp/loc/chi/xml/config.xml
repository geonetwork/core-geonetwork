<setting label="OAI Provider" name="oai"/>
<setting label="Cachesize" name="system/oai/cachesize">
			The number of OAI resultSets that can be stored in the Cache.
		</setting>
<setting label="Datesearch" name="system/oai/mdmode">
			Influences the behaviour of OAI when parameters from or until are supplied.
			If set to temporal extent, the temporal extent of the date described by the metadata is searched.
			If set to modification date, the date at which the metadata was last modified is searched
			
			Please note: restart GeoNetwork to make this change active.
		</setting>
<datesearchopt>
<dsopt label="temporal extent">1</dsopt>
<dsopt label="modification date">2</dsopt>
</datesearchopt>
<setting label="ResumptionToken Timeout " name="system/oai/tokentimeout">
			The number of seconds a OAI resultset stays in the cache and can be scrolled using the OAI resumptionToken
			
			Please note: restart GeoNetwork to make this change active.
		</setting>
<setting label="Proxy" name="proxy"/>
