<fields>
  <field name="gmx:Anchor">
    <suggest name="xlink:href"/>
  </field>
  <field name="gmx:scope">
    <suggest name="gco:CharacterString"/>
    <suggest name="gmx:FileName"/>
  </field>
  <field name="gmx:FileName">
    <suggest name="src"/>
  </field>

  <field name="gfc:FC_FeatureCatalogue">
    <suggest name="gmx:name"/>
    <suggest name="gmx:scope"/>
    <suggest name="gmx:fieldOfApplication"/>
    <suggest name="gmx:versionNumber"/>
    <suggest name="gmx:versionDate"/>
    <suggest name="gfc:functionalLanguage"/>
    <suggest name="gfc:producer"/>
  </field>

  <field name="gmd:CI_ResponsibleParty">
    <suggest name="gmd:individualName"/>
    <suggest name="gmd:organisationName"/>
    <suggest name="gmd:positionName"/>
    <suggest name="gmd:contactInfo"/>
  </field>

  <field name="gmd:contactInstructions">
    <suggest name="gco:CharacterString"/>
    <suggest name="gmx:FileName"/>
  </field>

  <field name="gfc:FC_FeatureType">
    <suggest name="gfc:typeName"/>
  </field>

  <field name="gfc:listedValue">
    <suggest name="gfc:FC_ListedValue"/>
  </field>

  <field name="gfc:FC_ListedValue">
    <suggest name="gfc:code"/>
  </field>

  <field name="gfc:FC_AssociationRole">
    <suggest name="gfc:definition"/>
  </field>

  <field name="gfc:constrainedBy">
    <suggest name="gfc:FC_Constraint"/>
  </field>

  <field name="gfc:FC_FeatureAttribute">
    <suggest name="gfc:code"/>
    <suggest name="gfc:memberName"/>
    <suggest name="gfc:code"/>
    <suggest name="gfc:definition"/>
    <suggest name="gfc:valueType"/>
    <suggest name="gfc:featureType"/>
  </field>

  <field name="gfc:code">
    <suggest name="gco:CharacterString"/>
    <suggest name="gmx:Anchor"/>
  </field>

  <field name="gfc:inheritsTo">
    <suggest name="gfc:FC_InheritanceRelation"/>
  </field>

  <field name="gfc:inheritsFrom">
    <suggest name="gfc:FC_InheritanceRelation"/>
  </field>

  <field name="gfc:code">
    <suggest name="gco:CharacterString"/>
    <suggest name="gmx:Anchor"/>
  </field>

</fields>
