      <identifier codeSpace="https://schemas.isotc211.org/19157/-2/mdq/1.0">bag</identifier>
    </Definition>
  </dictionaryEntry>
  <dictionaryEntry>
    <Definition gml:id="DQM_ValueStructure_set">
      <description>unordered collection of related items (objects or values) with no repetition (ISO 19107:2003)</description>
      <identifier codeSpace="https://schemas.isotc211.org/19157/-2/mdq/1.0">set</identifier>
    </Definition>
  </dictionaryEntry>
  <dictionaryEntry>
    <Definition gml:id="DQM_ValueStructure_sequence">
