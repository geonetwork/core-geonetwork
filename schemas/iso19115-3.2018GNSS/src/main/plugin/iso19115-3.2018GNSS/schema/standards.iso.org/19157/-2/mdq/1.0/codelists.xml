      <gco:CharacterString>ISO TC211 Metadata Standards</gco:CharacterString>
   </cat:fieldOfApplication>
   <cat:versionNumber>
      <gco:CharacterString>1.0</gco:CharacterString>
   </cat:versionNumber>
