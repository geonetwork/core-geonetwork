:Checksum_ShapeCV
    a sh:NodeShape ;
    sh:property [
        sh:node :ChecksumAlgorithmRestriction ;
        sh:nodeKind sh:IRI ;
        sh:path spdx:algorithm
    ] ;
    sh:targetClass spdx:Checksum.

