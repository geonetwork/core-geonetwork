<test>
    <request url="/en/xml.harvesting.get">
        <params>
            <id>${id}</id>
        </params>
    </request>

    <response>
		<node id="${id}" type="geonetwork">
			<site>
				<name>${name}</name>
				<uuid>${uuid}</uuid>
				<account>
					<use>false</use>
					<username/>
					<password/>
				</account>
				<host>${host}</host>
				<port>80</port>
				<servlet>${servlet}</servlet>
			</site>
			<content>
				<validate>false</validate>
				<importxslt>none</importxslt>
			</content>
			<options>
				<every>90</every>
				<oneRunOnly>false</oneRunOnly>
				<status>active</status>
			</options>
			<searches/>
			<groupsCopyPolicy/>
			<info>
				<lastRun/>
				<running>false</running>
			</info>
		</node>

    </response>
</test>
